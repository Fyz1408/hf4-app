<?xml version="1.0" encoding="UTF-8"?>

<Shell
    x:Class="hf4_app.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:views="clr-namespace:hf4_app.Views"
    Shell.FlyoutBehavior="Disabled">

    <TabBar >
        <Tab Title="Hjem" Icon="home.png">
            <ShellContent
            Title="Hjem"
            ContentTemplate="{DataTemplate views:FrontPage}"
            Route="FrontPage" />
        </Tab>
        <Tab Title="QR-scanner" Icon="qr.png">
            <ShellContent
            Title="QR-scanner"
            ContentTemplate="{DataTemplate views:QrScannerView}"
            Route="qrScannerView" />
        </Tab>
    </TabBar>
</Shell>